							"mode": "raw",
							"raw": "{\n  \"code\": \"your_authorization_code\",\n  \"clientId\": \"your_client_id\",\n  \"redirectUri\": \"https://stgitrade.cgsi.com.sg/portal/\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/sso/api/v1/token",
							"host": ["{{base_url}}"],
							"path": ["sso", "api", "v1", "token"]
						},
						"description": "Exchange authorization code for access and refresh tokens"
					},
					"response": []
